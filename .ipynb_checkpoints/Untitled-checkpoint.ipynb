   "source": [
    "from xlwt import Workbook\n",
    "import xlwt\n",
    "book = Workbook()\n",
    "sheet1 = book.add_sheet('Sheet 1')\n",
    "for i in range(0, 100):\n",
    "    st = xlwt.easyxf('pattern: pattern solid;')\n",
    "    st.pattern.pattern_fore_colour = i\n",
    "    sheet1.write(i % 24, i // 24, 'Test text', st)\n",
    "book.save('simple.xls')"
   ]
